{
    "name": "inspiredminds/contao-ip-login",
    "description": "Contao extension to allow automatic IP based front end login.",
    "type": "contao-bundle",
    "homepage": "https://github.com/inspiredminds/contao-ip-login",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Fritz Michael Gschwantner",
            "homepage": "http://www.inspiredminds.at",
            "email": "fmg@inspiredminds.at",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/inspiredminds/contao-ip-login/issues",
        "source": "https://github.com/inspiredminds/contao-ip-login"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/fritzmg"
        }
    ],
    "require": {
        "php": ">=8.1",
        "contao/core-bundle": "^5.0",
        "symfony/config": "^6.4 || ^7.1",
        "symfony/dependency-injection": "^6.4 || ^7.1",
        "symfony/expression-language": "^6.4 || ^7.1",
        "symfony/http-foundation": "^6.4 || ^7.1",
        "symfony/http-kernel": "^6.4 || ^7.1",
        "symfony/security-core": "^6.4 || ^7.1",
        "symfony/security-http": "^6.4 || ^7.1"
    },
    "autoload": {
        "psr-4": {
            "InspiredMinds\\ContaoIpLoginBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "InspiredMinds\\ContaoIpLoginBundle\\ContaoManager\\Plugin"
    },
    "require-dev": {
        "contao/easy-coding-standard": "^6.0",
        "contao/rector": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "php-http/discovery": false,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
